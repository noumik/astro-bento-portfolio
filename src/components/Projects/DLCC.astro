---
import ProjectCard from "../ProjectCard/index.astro";
import img from '../../lib/dlcc.jpg';
import skillicon1 from '../../lib/icons/SQL.png';
import skillicon2 from '../../lib/icons/Python-Light.svg';
import skillicon3 from '../../lib/icons/QGIS.png';

const image = [
  { img: img },
  { skillicon1: skillicon1 },
  { skillicon2: skillicon2 },
  { skillicon3: skillicon3 },
]
---

<ProjectCard colSpan="md:col-span-1" rowSpan="md:row-span-2" img={image[0]?.img?.src} backpos="center" opac="0.4" skillicon1={image[1]?.skillicon1?.src} skillicon2={image[2]?.skillicon2?.src} skillicon3={image[3]?.skillicon3?.src} desc='"Developed forecasting models to guide political strategy in nationwide state races"' href="https://noumik.weebly.com/dlcc-analysis.html">
  <div class="flex justify-start w-full items-start mb-2">
    <div class="flex flex-col">
      <h2 class="text-xl font-bold m-0 z-20">DLCC State Elections Analysis</h2>
      <h2></h2>
      <a href="https://sive.rs/nowff" target="_blank">
      </a>
    </div>
  </div>
</ProjectCard>
