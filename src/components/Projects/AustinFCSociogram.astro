---
import ProjectCard from "../ProjectCard/index.astro";
import img from '../../lib/austinfcsoc.jpg';
import skillicon1 from '../../lib/icons/VBA.webp';

const image = [
  { img: img },
  { skillicon1: skillicon1 },
]
---

<ProjectCard colSpan="md:col-span-1" rowSpan="md:row-span-2" img={image[0]?.img?.src} backpos="bottom" opac="0.7" skillicon1={image[1]?.skillicon1?.src} desc='"Visualized relationships among and between players and coaches in a user-friendly manner"' href="/blog/austinfcsociogram">
  <div class="flex justify-start w-full items-start mb-2">
    <div class="flex flex-col">
      <h2 class="text-xl font-bold m-0 z-20">Austin FC</h2>
      <h2>Sociogram Project</h2>
      <a href="https://sive.rs/nowff" target="_blank">
      </a>
    </div>
  </div>
</ProjectCard>
